(0)== word\n","\n","def isUpper(word):\n","  \"\"\"\n","    USA -> true but U.S.A -> false\n","  \"\"\"\n","  match = re.match(\"[A-Z]+\", word)\n","  if match == None: return False\n","  return match.group(0)== word\n","\n","def isLower(word):\n","  return word.islower()\n","\n","def isDigit(word):\n","  return word.isdigit()\n","\n","def isAlpha(word):\n","  \"\"\"\n","    an_com -> true; b2b -> false\n","  \"\"\"\n","  match = re.match(\"[A-Za-z_]+\", word)\n","  if match == None: return False\n","  return match.group(0)== word\n","\n","def isDate(word):\n","  \"\"\"\n","    12/3; 3-3-2003\n","  \"\"\"\n","  match = re.match(\"([\\d]+[-/])+[\\d]+\", word)\n","  if match == None: return False\n","  return match.group(0)== word\n","\n","def isCode(word):\n","  \"\"\"\n","    match : \"12B\", \"A12B\", \"2b\", ...\n","  \"\"\"\n","  match = re.match(\"([\\d]+[a-z A-Z]+) | ([a-z A-Z]+[\\d]+)\", word)\n","  return match != None\n","\n","\n","def isAbbr(word):\n","  \"\"\"\n","    match: \"U.A.A\", \"Th.\", \"T.\", \"Th.A\"; not match: \"Th.a\", \n","  \"\"\"\n","  match = re.match(\"([A-Z][a-z]*\\.)+[A-Z]?\", word)\n","  if match == None: return False\n","  return match.group(0)== word\n","\n","def isNumber(word):\n","  \"\"\"\n","   match: \"1.000\", \"1,000\", \"1.000.000\"\n","  \"\"\"\n","  match = re.match(\"([\\d]+[\\.,]?)+[\\d]*\", word)\n","  if match == None: return False\n","  return match.group(0)== word \n","\n","def isOutPunct(word):\n","  return word in \".?!\"\n","\n","def isInPunct(word):\n","  return word in \",();:\\\"\\'/\\\\\"\n","  \n","def getBrownEmbed(word, len):\n","  word = word.lower()\n","  if (word in wordDict): return wordDict[word][0:len]\n","  else: return \"0\"*len\n","getBrownEmbed(\"tớ\", 10)"],"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"application/vnd.google.colaboratory.intrinsic+json":{"type":"string"},"text/plain":["'0011110010'"]},"metadata":{"tags":[]},"execution_count":21}]},{"cell_type":"code","metadata":{"id":"MvBu_Kk4ilaV"},"source":["def word2features(sent, i):\n","  \"\"\"\n","    convert i-th word in sent into dictionary of features\n","  \"\"\"\n","  word = sent[i][0]\n","  features = {\n","      'bias': 1.0,\n","      'i.lower': word.lower(),\n","      'i.isupper': isUpper(word),\n","      'i.istitle': isTitle(word),\n","      'i.iscapital': isCapital(word),\n","      'i.isbigram': isBigram(word),\n","      'i.isdigit': isDigit(word),\n","      'i.isalpha': isAlpha(word),\n","      'i.isdate': isDate(word),\n","      'i.iscode': isCode(word),\n","      'i.isabbr': isAbbr(word),\n","      'i.isnumber': isNumber(word),\n","      'i.isoutpunct': isOutPunct(word),\n","      'i.isinpunct': isInPunct(word),\n","      'i.postag': sent[i][2],\n","      'i.br_embed_20':getBrownEmbed(word, 20),\n","      'i.br_embed_25':getBrownEmbed(word, 30),\n","      'i.br_embed_30':getBrownEmbed(word, 36),\n","      'i.br_embed_40':getBrownEmbed(word, 40)\n","  }\n","\n","  if(i>0):\n","    word = sent[i-1][0]\n","    features.update({\n","        '(i-1).lower': word.lower(),\n","        '(i-1).isupper': isUpper(word),\n","        '(i-1).istitle': isTitle(word),\n","        '(i-1).iscapital': isCapital(word),\n","        '(i-1).isbigram': isBigram(word),\n","        '(i-1).isdigit': isDigit(word),\n","        '(i-1).isalpha': isAlpha(word),\n","        '(i-1).isdate': isDate(word),\n","        '(i-1).iscode': isCode(word),\n","        '(i-1).isabbr': isAbbr(word),\n","        '(i-1).isnumber': isNumber(word),\n","        '(i-1).isoutpunct': isOutPunct(word),\n","        '(i-1).isinpunct': isInPunct(word),\n","        '(i-1).postag': sent[i-1][2],\n","        '(i-1).br_embed_20':getBrownEmbed(word, 20),\n","        '(i-1).br_embed_25':getBrownEmbed(word, 30),\n","        '(i-1).br_embed_30':getBrownEmbed(word, 36),\n","        '(i-1).br_embed_40':getBrownEmbed(word, 40)\n","    })\n","  else:\n","    features['BOS'] = True\n","  \n","\n","  if(i<len(sent)-1):\n","    word = sent[i+1][0]\n","    features.update({\n","        '(i+1).lower': word.lower(),\n","        '(i+1).isupper': isUpper(word),\n","        '(i+1).istitle': isTitle(word),\n","        '(i+1).iscapital': isCapital(word),\n","        '(i+1).isbigram': isBigram(word),\n","        '(i+1).isdigit': isDigit(word),\n","        '(i+1).isalpha': isAlpha(word),\n","        '(i+1).isdate': isDate(word),\n","        '(i+1).iscode': isCode(word),\n","        '(i+1).isabbr': isAbbr(word),\n","        '(i+1).isnumber': isNumber(word),\n","        '(i+1).isoutpunct': isOutPunct(word),\n","        '(i+1).isinpunct': isInPunct(word),\n","        '(i+1).postag': sent[i+1][2],\n","        '(i+1).br_embed_20':getBrownEmbed(word, 20),\n","        '(i+1).br_embed_25':getBrownEmbed(word, 30),\n","        '(i+1).br_embed_30':getBrownEmbed(word, 36),\n","        '(i+1).br_embed_40':getBrownEmbed(word, 40)\n","    })\n","  elif not isOutPunct(sent[i][0]):\n","    features['EOS'] = True\n","  \n","  if(i>1):\n","    word = sent[i-2][0]\n","    features.update({\n","        '(i-2).lower': word.lower(),\n","        '(i-2).isupper': isUpper(word),\n","        '(i-2).istitle': isTitle(word),\n","        '(i-2).iscapital': isCapital(word),\n","        '(i-2).isbigram': isBigram(word),\n","        '(i-2).isdigit': isDigit(word),\n","        '(i-2).isalpha': isAlpha(word),\n","        '(i-2).isdate': isDate(word),\n","        '(i-2).iscode': isCode(word),\n","        '(i-2).isabbr': isAbbr(word),\n","        '(i-2).isnumber': isNumber(word),\n","        '(i-2).isoutpunct': isOutPunct(word),\n","        '(i-2).isinpunct': isInPunct(word),\n","        '(i-2).postag': sent[i-2][2],\n","        '(i-2).br_embed_20':getBrownEmbed(word, 20),\n","        '(i-2).br_embed_25':getBrownEmbed(word, 30),\n","        '(i-2).br_embed_30':getBrownEmbed(word, 36),\n","        '(i-2).br_embed_40':getBrownEmbed(word, 40)\n","    })\n","  if i<len(sent)-2:\n","    word = sent[i+2][0]\n","    features.update({\n","       